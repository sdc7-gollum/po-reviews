{
  "name": "po-reviews",
  "version": "1.0.0",
  "description": "Reviews module of Project Overnight",
  "author": "Eric Song",
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build:dev": "webpack --watch",
    "csv1": "node --max-old-space-size=12288 database/dataGenReviews.js",
    "csv2": "node --max-old-space-size=12288 database/dataGenStays.js",
    "report:5min": "artillery run --output spec/reports/output5min.json spec/browse5min.yml",
    "report:dev": "artillery run --output spec/reports/outputDev.json spec/browseDev.yml",
    "seed": "node database/seeder.js",
    "start": "node server",
    "start:dev": "nodemon server -i client -w server",
    "stressTest:5min": "artillery run spec/browse5min.yml",
    "stressTest:dev": "artillery run spec/browseDev.yml",
    "test": "jest"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "jquery": "^3.5.1",
    "mongodb": "^3.5.7",
    "mongoose": "^5.9.10",
    "mongoose-sequence": "^5.2.2",
    "newrelic": "^6.8.0",
    "pg": "^8.2.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@shelf/jest-mongodb": "^1.1.5",
    "babel-loader": "^8.1.0",
    "babel-preset-airbnb": "^5.0.0",
    "css-loader": "^3.5.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "jest": "^26.0.1",
    "jest-config": "^26.0.1",
    "jest-environment-enzyme": "^7.1.2",
    "jest-enzyme": "^7.1.2",
    "nodemon": "^2.0.3",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
